<% if !@user_snippets.empty? %>
  <% @user_snippets.each do |snippet| %>
    <h5>Title</h5>
    <p><%= snippet.title %></p>
    <h5>Description</h5>
    <p><%= snippet.description %></p>
    <h5>Tag</h5>
    <p><%= snippet.tag %></p>
    <h5>Content</h5>
    <p><%= snippet.content %></p>
    
  <% end %>
<% else %>
  <p>There are currently no snippets.</p>
<% end %>